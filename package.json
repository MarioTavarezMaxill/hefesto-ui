{
  "name": "hefesto-ui",
  "keywords": [
    "Design System",
    "Web Components",
    "Stencil",
    "TailwindCSS",
    "Storybook",
    "Library",
    "Angular",
    "React",
    "Svelte",
    "Astro",
    "Web Components",
    "UI Components",
    "UI Library",
    "UI Design"
  ],
  "version": "1.0.18",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/hefesto-ui/hefesto-ui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/hefesto-ui/hefesto-ui.esm.js",
      "require": "./dist/hefesto-ui/hefesto-ui.cjs.js",
      "unpkg": "./dist/hefesto-ui/hefesto-ui.js"
    },
    "./badge-ui": {
      "import": "./dist/components/atoms/badge-ui.js",
      "types": "./dist/components/atoms/badge-ui.d.ts"
    },
    "./divider-ui": {
      "import": "./dist/components/atoms/divider-ui.js",
      "types": "./dist/components/atoms/divider-ui.d.ts"
    },
    "./skeleton-ui": {
      "import": "./dist/components/atoms/skeleton-ui.js",
      "types": "./dist/components/atoms/skeleton-ui.d.ts"
    },
    "./alert-ui": {
      "import": "./dist/components/atoms/alert-ui.js",
      "types": "./dist/components/atoms/alert-ui.d.ts"
    },
    "./indicator-ui": {
      "import": "./dist/components/atoms/indicator-ui.js",
      "types": "./dist/components/atoms/indicator-ui.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarioTavarezMaxill/hefesto-ui.git"
  },
  "files": [
    "dist/",
    "loader/",
    "!dist/**/*.map",
    "!dist/**/*.stories.*",
    "!dist/stats.html",
    "!dist/collection/**"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate.component": "stencil generate component",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token chpt_99cc90ef9862734 --exit-zero-on-changes"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@stencil/core": "^4.7.0",
    "@stencil/postcss": "^2.1.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-themes": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@storybook/web-components": "^8.1.11",
    "@storybook/web-components-vite": "^8.1.11",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.1.4",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "puppeteer": "^21.9.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "stencil-tailwind-plugin": "^1.8.0",
    "storybook": "^8.1.11",
    "storybook-addon-stencil": "^0.2.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  },
  "author": {
    "name": "Mario Tavarez",
    "email": "mariojosueitq@gmail.com",
    "url": "https://www.macoder.dev/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarioTavarezMaxill/hefesto-ui/issues"
  },
  "homepage": "https://github.com/MarioTavarezMaxill/hefesto-ui#readme"
}
